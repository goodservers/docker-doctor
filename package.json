{
  "name": "docker-doctor",
  "version": "0.1.7",
  "description": "Health checker for your docker containers on server. Zero downtime.",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "build": "export NODE_ENV=production && rm -rf ./lib && webpack --config webpack.config.js",
